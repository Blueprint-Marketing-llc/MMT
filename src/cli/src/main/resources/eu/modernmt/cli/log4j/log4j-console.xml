<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="%level" name="ModernMT" packages="">
    <Appenders>
        <Console name="Console" target="SYSTEM_ERR">
            <PatternLayout>
                <Pattern>%d %-5level %c [%t] %msg%n%throwable</Pattern>
            </PatternLayout>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="%level">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>